            <div class="container-fluid card-body card shadow mb-4">
                <!-- Page Heading -->
                <h1 class="h3 mb-0 text-gray-800">Manage Your Profile</h1>
                <p class="mb-4">Manage your profile information here</p>
                <hr><br>

                <div class="col-lg-6" >
                    <form class="user">
                        <div class="form-group">
                            <label>  Company Name (Enter your cable network name)</label>
                            <input type="city" class="form-control "form-control-user
                                   id="exampleInputCity" placeholder="World Wide Television...">

                        </div>
                        <div class="form-group">
                            <label>Address(Enter your office Address)</label>
                            <textarea id="message" name="phone" class="form-control" ></textarea>
                        </div>
                        <div class="form-group">
                            <label>City (Enter your office City)</label>
                            <input type="city" class="form-control "form-control-user
                                   id="exampleInputCity" placeholder="City">
                        </div>
                        <div class="form-group">
                            <label>State (Select your State)</label>
                            <select class="form-control "form-control-user >
                                <option>Maharashtra</option>
                                <option>Andhra Pradesh</option>
                                <option>Arunachal Pradesh</option>
                                <option>Assam</option>
                                <option>Bihar</option>
                                <option>Chhattisgarh</option>
                                <option> Gujarat</option>
                                <option>Madhya Pradesh</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <label>Pincode (Enter your pincode)</label>
                            <input type="pincode" class="form-control "form-control-user
                                   id="exampleInputPincode" placeholder="Pincode">
                        </div>
                        <div class="form-group">
                            <label>GST Number (Enter your GST number)</label>
                            <input type="pincode" class="form-control "form-control-user
                                   id="exampleInputPincode" placeholder="GST number">
                        </div>
                        <div class="form-group">
                            <label>Contact Number (Enter your contact number)</label>
                            <input type="" class="form-control "form-control-user
                                   id="exampleInput" placeholder="contact number">
                        </div>
                        <div class="text-center">
                            <a class="btn btn-facebook btn-user btn-block" href="#">Update</a>
                        </div>

                    </form>

                </div>
            </div>
